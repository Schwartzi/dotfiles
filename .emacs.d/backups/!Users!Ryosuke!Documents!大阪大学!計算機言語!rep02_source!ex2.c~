/* L3.4: C 言語の多分岐はどのようにコンパイルされるだろうか? */
int main()
{
  int x, y;

  x = 123;
  switch (x)
    {
    case 123:
      y = 1;
      break;
    case 234:
      y = 2;
      break;
    default:
      y = 3;
      break;
    }
  return (y);
}

/*
	.file	"ex2.c"
	.text
.globl main
	.type	main, @function
main:
	pushl	%ebp
	movl	%esp, %ebp
	subl	$16, %esp
	movl	$123, -8(%ebp)
	movl	-8(%ebp), %eax
	cmpl	$123, %eax
	je	.L3
	cmpl	$234, %eax
	je	.L4
	jmp	.L7
.L3:
	movl	$1, -4(%ebp)
	jmp	.L5
.L4:
	movl	$2, -4(%ebp)
	jmp	.L5
.L7:
	movl	$3, -4(%ebp)
.L5:
	movl	-4(%ebp), %eax
	leave
	ret
	.size	main, .-main
	.ident	"GCC: (Debian 4.4.5-8) 4.4.5"
	.section	.note.GNU-stack,"",@progbits
 */
