#include <stdio.h>

void main () {

    int ptb = 0;
    double r[8] = {3200000,
                1600000,
                800000,
                400000,
                200000,
                100000,
                50000,
                25000};
    int i, j;
    double tmp;

    unsigned char mask = 0x01;

    for (ptb = 0; ptb < 256; ptb++) {
        tmp = 0;
        for (i = 0; i < 8; i ++) {
            if (!((ptb >> i) & mask)) tmp += (1 / r[i]);
        }
        printf("%4d, %f\n", ptb, 1/tmp);
    }

};
